tocTab[tocTab.length] = new Array("0.3.2.3:4", "", "«Segurança» RNF03: O sistema deve validar a entrada de dados antes de serem processados e deve validar dados, conteúdo, tamanho, domínio, regras, etc no lado servidor", "EARoot/EA3/EA2/EA3/EA160.htm", "19.png", "E291", "0", "", "Requirement0","{043CD504-33E9-42ab-87E6-F0DDFC3C7659}");
tocTab[tocTab.length] = new Array("0.3.2.3:5", "", "«Segurança» RNF05: O sistema deve estar implementado em uma arquitetura disposta em camadas e seus componentes distribuídos. O banco de dados deverá estar segregado das aplicações", "EARoot/EA3/EA2/EA3/EA161.htm", "19.png", "E430", "0", "", "Requirement0","{5E43B908-5C6D-429d-901B-E14E900DC8ED}");
tocTab[tocTab.length] = new Array("0.3.2.3:6", "", "«Segurança» RNF06: O sistema deve manter log de acesso ao sistemas e informações pessoais/sensíveis", "EARoot/EA3/EA2/EA3/EA162.htm", "19.png", "E293", "0", "", "Requirement0","{79216720-9CB6-48c9-8E1C-A3D3A8B7D860}");
